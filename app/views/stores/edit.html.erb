<%= form_for @store do |f| %>
<h2>Editar Tienda</h2>
  <div>
     <%= f.label :name %>
     <%= f.text_field :name, :required => true %>
  </div>
  <div>
     <%= f.label :street %>
     <%= f.text_field :street, :required => true %>
  </div>
  <div>
     <%= f.label :number %>
     <%= f.number_field :number, :required => true %>
  </div>
  <div>
     <%= f.label :zip_code %>
     <%= f.number_field :zip_code, :required => true %>
  </div>
  <div>
     <%= f.label :city %>
     <%= f.text_field:city, :required => true %>
  </div>
  <div>
     <%= f.label :country %>
     <%= f.text_field :country, :required => true %>
  </div>
  <div>
     <%= f.label :products %>
     <%= f.select :products,options_from_collection_for_select(Product.all.order(:name), :name, :name, f.object.products)%>
  </div>
  <div>
     <%= f.submit %>
  </div>
<% end %>
